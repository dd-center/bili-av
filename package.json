{
  "name": "bili-av",
  "version": "1.0.0",
  "description": "Av Utils for Bilibili.",
  "author": "Il Harper",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "commit": "git cz"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix",
    "*.{css,vue}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cross-env": "^5.2.0",
    "element-ui": "^2.4.11",
    "koa": "^2.6.2",
    "koa-body": "^4.1.1",
    "koa-router": "^8.0.8",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "git-cz": "^4.3.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "stylelint": "^10.1.0"
  }
}
